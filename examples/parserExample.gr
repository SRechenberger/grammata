program ParserExample 
with
    var s := nums;
    var i := 0;
begin
    query nums() for X ?- 
        X :=: 1; 
        X :=: 2;
        X :=: 3;
        X :=: 4
    end

    func main() 
    with
        var x := 0;
    does
        while $x <= $s do
            x := $x + 1;
        end
        return $x;
    end
end
