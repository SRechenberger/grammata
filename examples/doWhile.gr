program{
    var x := 10;
    var bla := func (var a, var b) { 
        var toReturn := 0;

        do{
            if (a >= b) then {
                return toReturn;
            } else {
                toReturn := toReturn + b;
                b := b - a;
            };
        } while (a < b);

        return toReturn;
    };

    return x + bla(4,3);
}