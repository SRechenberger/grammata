program{
    func ggT := func(num a, num b){
        if (a == 0) then {
            return b;
        } else {
            while (0 != b){
                if (a > b) then {
                    a := a - b;
                } else {
                    b := b - a;
                };
            };
        };
        return a;
    };

    return ggT(412,12);
}